{
  "manifest_version": 2,
  "name": "Gmail send voice record",
  "short_name": "Gmail Voice Record",
  "description": "",
  "version": "0.0.1",
  "permissions": ["activeTab", "tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*", "*://inbox.google.com/*"],
      "js": [
        "resources/inboxsdk.js",
        "resources/jquery.js",
        "content.bundle.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": { "scripts": ["background.bundle.js"] },
  "icons": {
    "128": "resources/128.png"
  },
  "web_accessible_resources": ["resources/*"]
}
